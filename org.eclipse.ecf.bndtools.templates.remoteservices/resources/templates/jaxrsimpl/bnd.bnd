-buildpath: \
	osgi.annotation;version=7.0,\
	osgi.core;version=7.0,\
	osgi.cmpn;version=7.0,\
	$api_package$;version=latest,\
	jakarta.ws.rs-api

-testpath: \
	\${junit},\
	\${mockito},\
	\${mockito-deps}

$if(javaLevel)$
javac.source: $javaLevel$
javac.target: $javaLevel$
$endif$

Bundle-Version: 1.0.0.\${tstamp}
Export-Package: $projectName$